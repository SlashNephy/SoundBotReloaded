{
  "name": "soundbot-reloaded",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/SoundBotReloaded",
  "scripts": {
    "build": "tsc",
    "start": "ts-node main.ts",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@discordjs/opus": "0.10.0",
    "@discordjs/rest": "1.7.1",
    "@discordjs/voice": "0.13.0",
    "@slashnephy/typescript-extension": "0.2.3",
    "discord-api-types": "0.37.119",
    "discord.js": "14.6.0",
    "dotenv": "16.4.7",
    "ffmpeg-static": "5.2.0",
    "levenshtein-edit-distance": "3.0.1",
    "sodium-native": "3.4.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.3.186",
    "@slashnephy/prettier-config": "1.0.73",
    "@types/node": "22.13.5",
    "@types/prettier": "3.0.0",
    "concurrently": "9.1.2",
    "eslint": "8.57.1",
    "prettier": "3.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.8.7",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
