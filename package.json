{
  "name": "soundbot-reloaded",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/SlashNephy/SoundBotReloaded",
  "scripts": {
    "build": "tsc",
    "start": "ts-node main.ts",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/rest": "1.5.0",
    "@discordjs/voice": "0.13.0",
    "@slashnephy/typescript-extension": "0.2.3",
    "discord-api-types": "0.37.35",
    "discord.js": "14.6.0",
    "dotenv": "16.0.3",
    "ffmpeg-static": "5.1.0",
    "levenshtein-edit-distance": "3.0.1",
    "sodium-native": "3.4.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.3.5",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/node": "18.14.5",
    "@types/prettier": "2.7.2",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "prettier": "2.8.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.4.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
